pipeline {
    agent any

    stages {
        stage('clone code') {
            steps {
                git 'https://github.com/Hema79/Development-Team-Repo.git'
            }
        }
          stage('build application code') {
            steps {
                sh 'mvn package -f pom.xml'
            }
        }
        stage('uploade code to nexus') {
            steps {
                sh 'mvn package -f pom.xml'
            }
        }
    }
}
